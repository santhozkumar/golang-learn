package main

import (
	"fmt"
	"os"
)

func main() {
    for _, url := range os.Args[1:] {
        links, err = findLinks(url)

        if err != nil {
            fmt.Println(err)
        }
    }



}


func findLinks(url string) []string, error {
    return []string{"", ""}, error.New('got error')


}

